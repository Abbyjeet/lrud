{
  "name": "lrud",
  "version": "1.4.8",
  "description": "Left, Right, Up, Down. A spatial navigation library for devices with input via directional controls",
  "main": "src/index.js",
  "unpkg": "dist/lrud.min.js",
  "files": [
    "dist",
    "src"
  ],
  "author": {
    "name": "Stuart Williams",
    "url": "https://github.com/stuart-williams"
  },
  "license": "MIT",
  "keywords": [
    "tv",
    "navigation",
    "tv-apps",
    "react",
    "react-tv",
    "focus",
    "focus management",
    "spatial navigation",
    "smart tv"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:stuart-williams/lrud.git"
  },
  "devDependencies": {
    "jest": "^22.1.4",
    "rimraf": "^2.6.2",
    "rollup": "^0.53.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.2",
    "standard": "^10.0.3",
    "uglify-js": "^3.2.2"
  },
  "dependencies": {
    "object-assign": "^4.1.1",
    "tiny-emitter": "^2.0.2"
  },
  "scripts": {
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "clean": "rimraf lib",
    "lint": "standard lrud.js",
    "pretest": "npm run lint && npm run build",
    "test": "jest",
    "minify": "uglifyjs dist/lrud.js --compress --mangle --output dist/lrud.min.js",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "postbuild": "npm run minify"
  }
}
