{
  "name": "lrud",
  "version": "1.0.1",
  "main": "lib/lrud.js",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.53.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "sinon": "^4.1.3",
    "standard": "^10.0.3",
    "uglify-js": "^3.2.2"
  },
  "scripts": {
    "clean": "rimraf lib",
    "lint": "standard src/**/*.js",
    "test": "npm run lint && mocha --require babel-register",
    "minify": "uglifyjs lib/lrud.js --compress --mangle --output lib/lrud.min.js",
    "prebuild": "npm test && npm run clean",
    "build": "rollup -c",
    "postbuild": "npm run minify",
    "start:react": "cd examples/react && yarn && npm start",
    "start:ssr": "cd examples/ssr && yarn && npm start"
  }
}
