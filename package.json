{
  "name": "lrud",
  "version": "2.2.1",
  "description": "Left, Right, Up, Down. A spatial navigation library for devices with input via directional controls",
  "main": "dist/index.js",
  "author": {
    "name": "Stuart Williams",
    "url": "https://github.com/stuart-williams"
  },
  "license": "MIT",
  "keywords": [
    "tv",
    "navigation",
    "tv-apps",
    "react",
    "react-tv",
    "focus",
    "focus management",
    "spatial navigation",
    "smart tv"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:stuart-williams/lrud.git"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/node": "^9.4.7",
    "jest": "^22.1.4",
    "ts-jest": "^22.4.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "scripts": {
    "test": "jest --no-cache",
    "start": "tsc -w",
    "build": "tsc"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/*.+(ts|tsx|js)"
    ]
  }
}
